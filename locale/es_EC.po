msgctxt "error:account.liquidation.tax:"
msgid ""
"You can not add line \"%(line)s\" to liquidation \"%(liquidation)s\" because"
" it is posted, paid or canceled."
msgstr ""
"No puede agregar la linea \"%(line)s\" a la liquidación \"%(liquidation)s\" "
"porque está contabilizada, pagada o cancelada."

msgctxt "error:account.liquidation.tax:"
msgid ""
"You can not create liquidation \"%(liquidation)s\" on company "
"\"%(liquidation_company)s\" using account \"%(account)s\" from company "
"\"%(account_company)s\"."
msgstr ""
"No puede crear la liquidación \"%(liquidation)s\" en empresa "
"\"%(liquidation_company)s\" usando el código \"%(account)s\" de la empresa "
"\"%(account_company)s\"."

msgctxt "error:account.liquidation.tax:"
msgid ""
"You can not create liquidation \"%(liquidation)s\" on company "
"\"%(liquidation_company)s\" using base tax code \"%(base_code)s\" from "
"company \"%(base_code_company)s\"."
msgstr ""

msgctxt "error:account.liquidation.tax:"
msgid ""
"You can not create liquidation \"%(liquidation)s\" on company "
"\"%(liquidation_company)s\" using tax code \"%(tax_code)s\" from company "
"\"%(tax_code_company)s\"."
msgstr ""
"No puede crear la liquidación \"%(liquidation)s\" en empresa "
"\"%(liquidation_company)s\" usando el código \"%(tax_code)s\" de la empresa "
"\"%(tax_code_company)s\"."

msgctxt "error:account.liquidation.tax:"
msgid ""
"You can not modify tax \"%(tax)s\" from liquidation \"%(liquidation)s\" "
"because it is posted or paid."
msgstr ""
"No puede modificar el impuesto \"%(tax)s\" de la liquidación "
"\"%(liquidation)s\" porque está contabilizada o pagada."

msgctxt "error:account.liquidation:"
msgid ""
"There is no liquidation sequence for liquidation \"%(liquidation)s\" on the "
"period/fiscal year \"%(period)s\"."
msgstr ""
"No hay secuencia de liquidación para la liquidación \"%s(liquidation)s\" en "
"el período/año fiscal \"%(period)s\""

msgctxt "error:account.liquidation:"
msgid "You can not delete a liquidation that is posted!"
msgstr "No puede eliminar una liquidación que está contabilizada."

